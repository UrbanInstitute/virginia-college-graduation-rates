{
  "name": "college-metrics-dashboard",
  "version": "0.1.0",
  "homepage": "https://UI-Research.github.io/college-metrics-dashboard",
  "private": true,
  "engines": {
    "node": ">=12.10.0"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "history": "^4.10.1",
    "http-server": "^0.12.1",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.3.0",
    "react-select": "^3.0.5",
    "react-tooltip": "^3.11.1",
    "read-excel-file": "^4.0.2",
    "recharts": "^1.7.1",
    "yarn": "^1.21.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "object-curly-spacing": [
        "error",
        "never"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "replace-in-file": "^5.0.2"
  }
}
